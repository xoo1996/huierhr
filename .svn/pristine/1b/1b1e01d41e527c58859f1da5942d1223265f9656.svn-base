ord04_000=update tblfoldetail set fdtsqnt=:fdtsqnt!, fdtdprc=:fdtdprc!,fdtsta=:fdtsta!,fdtnt=:fdtnt! where fdtfno=:fdtfno! and fdtpid=:fdtpid!
ord04_001=insert into tblfoldetail (fdtfno,fdtcltid,fdtpid, fdtprc, fdtqnt, fdtsqnt, fdtrqnt, fdtdprc, fdtnt,fdtcltnm,fdtodt,fdtono) select :fdtcltid!,fdtcltid,fdtpid, fdtprc, '-1', '-1', '-1', fdtdprc, :fdtnt!,fdtcltnm,:fdtodt!,:fdtfno! from tblfoldetail where fdtfno=:fdtfno!
ord04_002=insert into tblfolio (folno, folctid, foldt, folopr, folnt, folsta, foltype,folsno, foldes, folway, folsdt, folsopr, folpdt) select :folnt!, folctid, foldt, :folopr!, folnt, folsta, foltype,folsno, foldes, folway, :folsdt!, folsopr, folpdt from tblfolio where folno=:folno!
