sta01_000=select d.*,h.*,c.ictaddr as foladdr,decode(d.fdtcltid,null,fdtcltnm,(select c.ictnm from tblindclient c where c.ictid=d.fdtcltid)) as cltnm, g.gctnm as folctnm, p.pdtdiscount,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join tblgrpclient g on g.gctid=h.folctid  left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient c on c.ictid=d.fdtcltid where h.folno like :folno! and g.gctnm like :folctnm! and h.folsta like :folsta! and h.foltype like :foltype! and h.foldt>=:start! and h.foldt<=:end!order by h.foldt

