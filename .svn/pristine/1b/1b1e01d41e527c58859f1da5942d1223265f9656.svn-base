ord02_000=select h.*,o.bsc012 as foloprnm,g.gctnm as folctnm from tblfolio h left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid where h.folno like :folno! and h.folctid like :folctid! and h.foldt like :foldt!
ord02_001=select h.*,o.bsc012 as foloprnm,g.gctnm as folctnm from tblfolio h left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid where h.folno like :folno! and h.folctid like :folctid! and h.foldt like :foldt! and h.folsta in ('waiting','finishPart') and h.foltype='normal'
ord02_002=select d.*,h.*,o.bsc012 as folsoprnm,g.gctnm,p.pdtnm as pdtnm,p.pdtid as pdtid from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folsopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folsta='finish' and h.folctid=:folctid! and h.folsdt>=:start! and h.folsdt<=:end! and d.fdtpid like :pdtid! and h.foltype=:foltype! order by h.folsdt 
#ord02_003=select d.*,h.*,g.gctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folno like :folno! and h.folctid like :folctid! and h.foldt>=:start! and h.foldt<=:end! and h.folsta=:folsta! and h.foltype<>'normal'
ord02_003=select d.*,h.*,g.gctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where  h.folsta=:folsta! and h.folno like :folno! and h.folctid like :folctid! and h.foldt>=:start! and h.foldt<=:end! and h.foltype<>'normal' and h.foltype=:foltype!
#ord02_004=select d.*,h.*,o.bsc012 as foloprnm,decode(d.fdtcltid,null,fdtcltnm,(select c.ictnm from tblindclient c where c.ictid=d.fdtcltid)) as cltnm,g.gctnm as folctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folno like: folno! and h.folctid like: folctid! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! order by h.foldt
#ord02_004=select d.*,h.*,o.bsc012 as foloprnm,g.gctnm as folctnm,i.ictnm as cltnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on d.fdtcltid=i.ictid where i.ictgctid=:ictgctid! and h.folno like :folno! and g.gctnm like :gctnm! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! and h.folsta=:folsta! order by h.foldt

#ord02_004=select distinct d.fdtfno,m.tmksid,d.fdtqnt,d.fdtsqnt,d.fdtdprc,d.fdtnt,d.fdtcltnm as cltnm,h.*,o.bsc012 as foloprnm,g.gctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left join tblmaking m on h.folno=m.tmkfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on d.fdtcltid=i.ictid where h.folctid=:ictgctid! and h.folno like :folno! and g.gctnm like :gctnm! and d.fdtcltnm like :cltnm! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! and h.folsta=:folsta! and h.folsno=:folsno! order by h.foldt
ord02_004=select distinct d.fdtfno,m.tmksid,nvl(nvl(m.tmkpdt,r.reppdate),e.TMEPDT) as tmkpdt,d.fdtpid,d.fdtqnt,d.fdtsqnt,d.fdtdprc,d.fdtnt,d.fdtcltnm as cltnm,h.*,o.bsc012 as foloprnm,g.gctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left join tblmaking m on h.folno=m.tmkfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on d.fdtcltid=i.ictid left outer join tblrep r ON h.folno=r.repfolid left outer join tblearmaking e on e.tmeno=h.folno where d.fdtpid=:fdtpid! and h.folctid=:ictgctid! and h.folno like :folno! and g.gctnm like :gctnm! and d.fdtcltnm like :cltnm! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! and h.folsta=:folsta! and h.folsno=:folsno! order by h.foldt
ord02_004_his=select distinct d.fdtfno, d.fdtpid, d.fdtqnt, d.fdtsqnt, d.fdtdprc, d.fdtnt, d.fdtcltnm as cltnm, h.*, o.bsc012 as foloprnm, g.gctnm, p.pdtnm from tblfoldetail@mylinke d left outer join tblfolio@mylinke h on h.folno=d.fdtfno left outer join sc05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on d.fdtcltid=i.ictid where d.fdtpid=:fdtpid! and h.folctid =:ictgctid! and h.folno like :folno! and g.gctnm like :gctnm! and d.fdtcltnm like :cltnm! and h.foldt >=:start! and h.foldt <=:end! and h.foltype=:foltype! and h.folsta =:folsta! and h.folsno =:folsno!
ord02_009=select distinct d.fdtfno,m.tmksid,d.fdtqnt,d.fdtsqnt,d.fdtdprc,d.fdtnt,d.fdtcltnm as cltnm,h.*,o.bsc012 as foloprnm,g.gctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left join tblmaking m on h.folno=m.tmkfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on d.fdtcltid=i.ictid where h.folctid=:ictgctid! and g.gctnm like :gctnm! and d.fdtcltnm like :cltnm! and h.folexdt>=:exstart! and h.folexdt<=:exend! and h.foltype=:foltype! and h.folsta in ('allocated','charged','finish','making','pass','qcing','recback','recoiled','recoiledhead','recpass','repairing','unallocated','waiting') and p.pdtid like :pdtid! and p.pdtnm like :pdtnm! order by h.foldt

ord02_005=select r.*,r.reppid as pdtid,p.pdtnm as pdtnm,g.gctnm as repgctnm from tblrep r left outer join tblproduct p on p.pdtid=r.reppid  left outer join tblgrpclient g on g.gctid=r.repgctid  where r.reppid=:pdtid!
ord02_006=select d.*,h.*,o.bsc012 as foloprnm,g.gctnm as folctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.foltype='unallocated' h.folno like :folno! and h.folctid like :folctid! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! order by h.foldt 
#ord02_007=select d.*,h.*,o.bsc012 as foloprnm,g.gctnm as folctnm,p.pdtnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folsta='unallocated' and h.folno like :folno! and h.folctid like :folctid! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! order by h.foldt
ord02_007=select distinct h.*,g.gctnm as folctnm from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folsta='unallocated' and h.folno like :folno! and h.folctid like :folctid! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! order by h.foldt

ord02_008=select distinct d.*,h.*,o.bsc012 as foloprnm,(case when s.aab300='\u60e0\u8033\u542c\u529b\u603b\u90e8' then s.aab300 else g.gctnm end)as gctnm,p.pdtnm,i.ictnm,i.ictid from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join SC05 o on o.bsc011=h.folopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid left outer join tblindclient i on h.folindctid=i.ictid left join sc01 s on h.folctid=s.bsc001 where h.folno like :folno! and d.fdtcltnm like:fdtcltnm! and g.gctnm like :gctnm! and i.ictnm like :ictnm! and h.foldt>=:start! and h.foldt<=:end! and h.foltype=:foltype! and h.folsta=:folsta! and h.folsta in ('commited','pass') order by h.foldt
ord02_010=select d.fdtfno,d.fdtcltid,d.fdtpid,d.fdtprc,(case when h.folsta='recpass' then -d.fdtqnt else d.fdtqnt end)as fdtqnt,(case when h.folsta='recpass' then -d.fdtsqnt else d.fdtsqnt end)as fdtsqnt,d.fdtcltnm, h.*,o.bsc012 as folsoprnm,g.gctnm,p.pdtnm as pdtnm,p.pdtid as pdtid from tblfoldetail d left outer join tblfolio h on h.folno=d.fdtfno left outer join sc05 o on o.bsc011=h.folsopr left outer join tblgrpclient g on g.gctid=h.folctid left outer join tblproduct p on p.pdtid=d.fdtpid where h.folsta in('finish','charged','recback','recpass','recoiled','recoiledhead') and h.folctid=:folctid! and h.folsdt>=:start! and h.folsdt<=:end! and d.fdtpid like :pdtid! and h.foltype=:foltype! and h.folsdt>=to_date('2014-01-01','yyyy-MM-dd') order by h.folsdt


