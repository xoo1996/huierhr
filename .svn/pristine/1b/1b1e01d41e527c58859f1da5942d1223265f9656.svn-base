ord06_000=select d.*,f.foltype,f.folno,m.tmksid, p.pdtnm,g.gctnm as folctnm,decode(d.fdtono,null,f.folsdt,(select h.folsdt from tblfolio h where h.folno=d.fdtono)) as folsdt,o.bsc012 as foloprnm from tblfoldetail d  left outer join tblfolio f on f.folno= d.fdtfno left outer join SC05 o on o.bsc011=f.folopr left outer join tblproduct p on d.fdtpid=p.pdtid left outer join tblgrpclient g on g.gctid=f.folctid left outer join tblmaking m on m.tmkfno= d.fdtono where d.fdtsqnt<0 and m.tmkcltid=:folctid!

