ord07_000=select t.ictid,t.ictgctid,t.ictnm,t.ictbdt,t.ictaddr,t.icttel,t.ictsrc,t.ictfrom,t.ictnt,(case when t.ictgender='0' then '\u5973' when t.ictgender='1'then '\u7537' end )as ictgender ,(case t.ictphis  when '0' then '\u672a\u6234\u8fc7' when '1' then '\u76d2\u5f0f' when '2' then '\u8033\u9053\u5f0f' when '3' then '\u8033\u80cc\u5f0f'end) as ictphis from tblindclient t where t.ictid=:ictid!
ord07_001=select t.ictid,t.ictgctid,t.ictnm,t.ictbdt,t.ictaddr,t.icttel,t.ictsrc,t.ictnt,t.ictfrom, t.ictgender ,t.ictphis from tblindclient t where t.ictid=:ictid!
ord07_002=select t.ictid,t.ictgctid,g.gctnm as ictgctnm,t.ictnm,t.ictbdt,t.ictaddr,t.icttel,t.ictsrc,t.ictfrom,t.ictnt, t.ictgender ,t.ictphis from tblindclient t,tblgrpclient g where t.ictgctid=g.gctid and t.ictid=:ictid!


