ord05_000=select d.*,f.foltype,f.folno,m.tmksid, p.pdtnm,g.gctnm as folctnm,decode(d.fdtono,null,f.folsdt,(select h.folsdt from tblfolio h where h.folno=d.fdtono)) as folsdt,o.bsc012 as foloprnm from tblfoldetail d  left outer join tblfolio f on f.folno= d.fdtfno left outer join SC05 o on o.bsc011=f.folopr left outer join tblproduct p on d.fdtpid=p.pdtid left outer join tblgrpclient g on g.gctid=f.folctid left outer join tblmaking m on (m.tmkfno=d.fdtfno  or m.tmkfno= d.fdtono) where d.fdtsqnt<0 and f.folctid=:folctid! and d.fdtpid=:pdtid! and d.fdtodt>=:start! and d.fdtodt<=:end! 
#ord05_001=select d.*,f.*,m.tmksid,f.folsdt as temp,o.bsc012 as foloprnm,p.pdtnm,g.gctnm as folctnm from tblfoldetail d left outer join tblfolio f on f.folno= d.fdtfno  left outer join SC05 o on o.bsc011=f.folopr left outer join tblmaking m on (m.tmkfno= d.fdtfno  or m.tmkfno= d.fdtono) left outer join tblproduct p on d.fdtpid= p.pdtid left outer join tblgrpclient g on g.gctid=f.folctid where f.foltype='make' and d.fdtcltnm like :cltnm! and m.tmksid=:sid!
ord05_001=select d.*,f.folno,f.folctid,f.foldt,f.folopr,f.folnt,f.folsta,f.foltype,f.folsno,f.foldes,f.folway,f.folsdt,f.folsopr,f.folpdt,m.tmksid,f.folsdt as temp,o.bsc012 as foloprnm,p.pdtnm,g.gctnm as folctnm from tblfoldetail d left outer join tblfolio f on f.folno= d.fdtfno  left outer join SC05 o on o.bsc011=f.folopr left outer join tblmaking m on (m.tmkfno= d.fdtfno  or m.tmkfno= d.fdtono) left outer join tblproduct p on d.fdtpid= p.pdtid left outer join tblgrpclient g on g.gctid=f.folctid where f.foltype='make' and d.fdtcltnm like :cltnm! and m.tmksid=:sid!
ord05_002=select d.*,f.foltype,f.folno,f.folischg,m.tmksid, p.pdtnm,g.gctnm as folctnm,decode(d.fdtono,null,f.folsdt,(select h.folsdt from tblfolio h where h.folno=d.fdtono)) as folsdt,o.bsc012 as foloprnm from tblfoldetail d  left outer join tblfolio f on f.folno= d.fdtfno left outer join SC05 o on o.bsc011=f.folopr left outer join tblproduct p on d.fdtpid=p.pdtid left outer join tblgrpclient g on g.gctid=f.folctid left outer join tblmaking m on (m.tmkfno=d.fdtfno  or m.tmkfno= d.fdtono) where d.fdtsqnt<0  and d.fdtnt='\u9000\u673a' and f.folischg=:folischg! and d.fdtcltnm like :cltnm! and f.folctid=:folctid! and d.fdtpid=:pdtid! and d.fdtodt>=:start! and d.fdtodt<=:end!
