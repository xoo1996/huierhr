# Set root logger level to DEBUG and its only appender to A1.
log4j.rootLogger=INFO, A1

#log4j.rootLogger=INFO, A1

log4j.logger.cdsc.system.publisher.report = DEBUG

# A1 is set to be a ConsoleAppender.
log4j.appender.A1=org.radf.plat.log.JmsLogAppender
#log4j.appender.A1=org.apache.log4j.ConsoleAppender
#log4j.appender.A2=com.lbs.sieaf.log.JmsLogAppender
#log4j.appender.A1=org.apache.log4j.FileAppender
#log4j.appender.A1.File=error.log 

#log4j.logger.cdsc.system.publisher.report=DEBUG, A@
#log4j.logger.cdsc.system.publisher.report=ERROR

# A1 uses PatternLayout.
log4j.appender.A1.layout=org.apache.log4j.PatternLayout
log4j.appender.A1.layout.ConversionPattern=%c %x - %m%n
#log4j.appender.A1.layout.ConversionPattern=%-4r %-5p %c %x - %m%n
#log4j.appender.A1.layout.ConversionPattern=%c : %m%n


