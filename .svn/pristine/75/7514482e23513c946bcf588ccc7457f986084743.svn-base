		<queryString>
			<![CDATA[select distinct e.dgnldprc,e.dgnldct,p.pdtnm,e.dgnrdprc,e.dgnrdct,
(case e.dgnpulwire when '0' then '左' when '1' then '右' else '双耳' END) dgnpulwire,
(case e.dgnairvent when '0' then '左' when '1' then '右' else '双耳' END) dgnairvent,
