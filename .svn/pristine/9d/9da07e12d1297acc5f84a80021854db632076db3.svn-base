	<subDataset name="dingzhi">
		<parameter name="folno" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT e.dgnldct,
  (
  CASE
    WHEN d.fdtmklr='0'
    THEN p.pdtnm
  END) AS lpdtnm,
  (
  CASE
    WHEN d.fdtmklr='1'
    THEN p.pdtnm
  END) AS rpdtnm,
  (
  CASE
    WHEN d.fdtmklr='0'
    THEN h.foldps
  END) AS lfoldps,
  (
  CASE
    WHEN d.fdtmklr='1'
    THEN h.foldps
  END) AS rfoldps,
  e.dgnldprc,
  e.dgnrdprc,
  e.dgnrdct,
  e.dgnpulwire,
  e.dgnairvent,
  e.dgnvoswitch,
  e.dgnsdmd,
  e.dgnlid,
  e.dgnrid,
  e.dgnid,
  e.dgndoc,
  e.dgnlfdtfno,
  e.dgnrfdtfno,
  h.foldps,
   h.isleft,
  h.isright,
  h.feeleft,
  h.feeright,
  h.delayleft,
  h.delayright,
  d.fdtmklr
FROM tbldiagnose e
LEFT JOIN tblproduct p
ON e.dgnlid=p.pdtid
LEFT JOIN tblfoldetail d
ON d.fdtfno=$P{folno}
LEFT JOIN tblfolio h
ON h.folno        =$P{folno}
WHERE e.dgnlfdtfno=$P{folno}
OR e.dgnrfdtfno   =$P{folno}]]>
		</queryString>
