	<queryString>
		<![CDATA[select distinct h.folctid 客户代码,g.gctnm 客户名称,g.gctaddr 联系地址,g.gctarea 区域,a.aaa103 所在省,i.ictnm 个人客户,p.pdtnm 商品名称,p.pdtprc 价格,
d.fdtsqnt 回款数,h.foldis 扣率,d.fdtprc 回款金额,h.folnt 备注,h.foltype 订单类型
from tblfoldetail d
left outer join tblindclient i on i.ictid=d.fdtcltid
left outer join tblfolio h on h.folno=d.fdtfno
left outer join tblproduct p on p.pdtid=d.fdtpid
left outer join tblgrpclient g on g.gctid=h.folctid
left outer join aa10 a on (a.aaa102=g.gctprovince and a.aaa100='GCTPROVINCE')
where h.folctid=$P{code}  and h.foltype <> 'normal' and h.folctid like 'A%' and h.folsta='charged'  and h.folchgdt <='' and h.folchgdt >=''
union
select distinct n.chggcltid 客户代码,g.gctnm 客户名称,g.gctaddr 联系地址,g.gctarea 区域,a.aaa103 所在省,i.ictnm 个人客户,p.pdtnm 商品名称,p.pdtprc 价格,
r.ncdqnt 回款数,r.ncddis 扣率,r.ncdmon 回款金额,r.ncdnt 备注,'normal' 订单类型
from tblnorchg n
left outer join tblnorchgdetail r on r.ncdid=n.chgid
left outer join tblindclient i on i.ictid=n.chgcltid
left outer join tblproduct p on p.pdtid=r.ncdpid
left outer join tblgrpclient g on g.gctid=n.chggcltid
left outer join aa10 a on (a.aaa102=g.gctprovince and a.aaa100='GCTPROVINCE')
where n.chggcltid=$P{code} and n.chgdt <='' and n.chgdt >= '']]>
	</queryString>
